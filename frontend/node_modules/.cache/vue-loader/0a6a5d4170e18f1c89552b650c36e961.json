eColor=\"item.color\"></todo>\n        </van-cell>\n\n        <!-- </li> -->\n      </van-list>\n      <div v-if=\"dataList.length > 0\" class=\"btn-list\">\n          <label>{{}}items left</label>\n          <van-button text=\"all\" type=\"default\" ></van-button>\n          <van-button type=\"default\" @click=\"changeActiveColor(2)\">active</van-button>\n          <van-button type=\"default\" @click=\"changeActiveColor(3)\">completed</van-button>\n          <a   @click=\"changeActiveColor(3)\">clear completed</a>\n        </div>\n      <!-- </!-->\n\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport Todo from \"./components/Todo.vue\"\nimport { List, Cell, Field, Button } from 'vant';\nexport default {\n  name: 'APP',\n  components: {\n    Todo,\n    [List.name]: List,\n    [Cell.name]: Cell,\n    [Field.name]: Field,\n    [Button.name]: Button,\n  },\n  data(){\n    return{\n      newTodo: \"\",\n      dataList: [],\n      colorList:['black','red','yellow','blue'],\n      activeColor:'black',\n    }\n  },\n  methods:{\n    submitTodo(){\n      if(!this.newTodo){\n        return\n      }\n      var noteItem = { color: this.activeColor, content: this.newTodo,iconStatus:'circle' }\n      this.dataList.push(noteItem);\n      this.newTodo = \"\";\n    },\n    changeActiveColor(index){\n     this.activeColor = this.colorList[index];\n    },\n    handleDel(index){\n      this.dataList.splice(index,1);\n    },\n    // getTdo(){\n    //   this.ajax(\"post\", 'www.baidu.com', data,(result)=>{\n    //     this.dataList = result.data;\n    //   })\n    // },\n    ajax(method,url,data,callback,flag ) {\n      var xhr = 'null';\n      if (window.XMLHttpRequest) {\n        xhr = new XMLHttpRequest();\n      }\n      method = method.toUpperCase();\n      if (method == 'GET') {\n        xhr.open('GET', url+'?'+data, flag);\n        xhr.send();\n        xhr.onreadystatechange = function () {\n          if (xhr.readyState == 4) {\n            if (xhr.status == 200) {\n              callback(xhr.responseText);             // xhr.onsponseText是字符串类型的json格式，需要转换成json格式。前面加个JSON.parse()\n            }\n          }\n        }\n      }else if(method == 'POST'){\n        xhr.open('post', url, flag);\n        xhr.setRequestHeader('Content-type','application/x-www-form-urlencoded');\n        xhr.send(data);\n        xhr.onreadystatechange = function () {\n          if (xhr.readyState == 4) {\n            if (xhr.state == 200) {\n              callback(xhr.responseText);\n            }\n          }\n        }\n      }\n    }\n  }\n}\n</script>\n\n\n<style scoped>\n#todo-div{\n  position: relative;\n  width: 50%;\n  margin: 5px;\n  top: 100px;\n  background: #fff;\n  left: 25%;\n  transform: translateX(-50%), translateY(-50%);\n}\n#input-box{\n  position: absolute;\n  width: 50%;\n  height: 40px;\n  top: 80%;\n  left: 25%;\n  transform: translateX(-50%), translateY(-50%);\n  display: flex;\n  flex-direction: column;\n}\n#todo-div .todo-list{\n  top: 50px;\n  max-height: 400px;\n  background: #ccc;\n  overflow: auto;\n}\nli {\n  list-style: none;\n  margin: 0 10px;\n}\n#todo-div #input-box{\nbottom: 20px;\nleft: 50%;\ntransform: translateX(-50%);\n}\n#input-box input{\n  background-color: #fff;\n  background-image: none;\n  border-radius: 4px;\n  border: 1px solid #dcdfe6;\n  box-sizing: border-box;\n  display: inline-block;\n  font-size: inherit;\n  height: 40px;\n  line-height: 40px;\n  outline: none;\n  padding: 0 15px;\n  transition: border-color .2s cubic-bezier(.645,.045,.355,1);\n  width: 100%;\n}\n.btn-list{\n  padding: 10px 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.btn-list button{\n  border: none;\n}\n.btn-list button:hover{\n  border: 1px solid #ebedf0;\n}\n.btn-list a:hover{\n  text-decoration: underline;\n  cursor: pointer;\n}\n</style>\n"]}]}