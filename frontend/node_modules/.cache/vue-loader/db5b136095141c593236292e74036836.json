ld v-model=\"username\" required clearable label=\"用户名\" right-icon=\"question-o\" placeholder=\"请输入用户名\" @click-right-icon=\"$toast('question')\"/>\n          <van-field v-model=\"password\" type=\"password\" label=\"密码\" placeholder=\"请输入密码\" required />\n          <van-button type=\"default\">默认按钮</van-button>\n          <van-button type=\"default\">默认按钮</van-button>\n        </van-cell-group>\n      </div>\n    </van-overlay>\n    <div class=\"todo-div\">\n      <van-icon name=\"arrow-down\" v-if=\"activeNum > 0\" class=\"select-icon\" @click=\"allCompleted()\"/>\n      <van-icon v-else name=\"plus\" class=\"select-icon\" />\n      <van-field v-model=\"newTodo\" @keyup.enter=\"submitTodo\" class=\"input-box\"></van-field>\n      <van-list class=\"todo-list\">\n        <van-cell v-for=\"(item, index) in dataList\" v-bind:key=\"item.id\" >\n          <todo :item=\"item\" :index=\"index\" @handleDel=\"handleDel\" @handleStatus=\"handleStatus\"></todo>\n        </van-cell>\n      </van-list>\n      <div v-if=\"activeNum > 0\" class=\"btn-list\">\n        <label v-if=\"activeNum == 1\">1 item left</label>\n        <label v-else>2 items left</label>\n        <van-button text=\"all\" type=\"default\" @click=\"changeToAll()\"></van-button>\n        <van-button text=\"active\" type=\"default\" @click=\"changeToActive()\"></van-button>\n        <van-button text=\"completed\" type=\"default\" @click=\"changeToComplelted()\"></van-button>\n        <a @click=\"clearCompleted()\">clear completed</a>\n      </div>\n    </div>\n\n  </div>\n</div>\n</template>\n\n<script>\nimport Todo from \"./components/Todo.vue\"\nimport { List, Cell, Field, Button, Icon, Overlay, NavBar } from \"vant\";\nexport default {\n  name: \"APP\",\n  components: {\n    Todo,\n    [List.name]: List,\n    [Cell.name]: Cell,\n    [Field.name]: Field,\n    [Button.name]: Button,\n    [Icon.name]: Icon,\n    [Overlay.name]: Overlay,\n    [NavBar.name]: NavBar,\n  },\n  data() {\n    return {\n      newTodo: \"\",\n      dataList: [],\n      allData: [],\n      activeList: [],\n      completedList: [],\n      activeNum: 0,\n      showLogin: false,\n      navTitle: \"\",\n      navButton: \"登录/注册\",\n    }\n  },\n  mounted() {\n    this.changeToAll();\n  },\n  methods:{\n    onClickRight() {\n        this.showLogin = true;\n    },\n    submitTodo() {\n      if(!this.newTodo) {\n        return\n      }\n      var noteItem = { content: this.newTodo, status: true }\n      this.allData.unshift(noteItem);\n      this.newTodo = \"\";\n      this.activeNum += 1;\n    },\n    handleDel(index) {\n      this.dataList.splice(index,1);\n    },\n    handleStatus(status, index){\n      this.dataList[index].status = status;\n      if (status) {\n        this.activeList.unshift(this.completedList[index]);\n        this.completedList.splice(index, 1);\n        this.activeNum += 1;\n      } else {\n        this.completedList.unshift(this.activeList[index]);\n        this.activeList.splice(index, 1)\n        this.activeNum -= 1;\n      }\n    },\n    changeToAll() {\n      this.allData = [{ content: \"TestActive\", status: true }, { content: \"TestCompleted\", status: false }, { content: \"TestActive\", status: true },{ content: \"TestCompleted\", status: false }, { content: \"TestActive\", status: true }, { content: \"TestCompleted\", status: false }];\n      this.dataList = this.allData;\n    },\n    changeToActive() {\n      this.activeList = [{ content: \"TestActive\", status: true }, { content: \"TestActive\", status: true }, { content: \"TestActive\", status: true }];\n      this.dataList = this.activeList;\n    },\n    changeToComplelted() {\n      alert(2)\n      this.completedList = [{ content: \"TestCompleted\", status: false }, { content: \"TestCompleted\", status: false }, { content: \"TestCompleted\", status: false }];\n      this.dataList = this.completedList;\n    },\n    allCompleted() {\n      return\n    },\n    clearCompleted() {\n      this.completedList = [];\n      for (var i = 0; i < this.dataList.length;) {\n        if (this.dataList[i].status == false) {\n          this.dataList.splice(i, 1);\n          continue\n        }\n        i++;\n      }\n    },\n  }\n}\n</script>\n\n<style scoped>\n.info-div {\n  position: relative;\n  width: 200px;\n  top: 100px;\n}\n.todo-div {\n  position: relative;\n  width: 50%;\n  margin: 5px;\n  top: 100px;\n  background: #fff;\n  left: 25%;\n  transform: translateX(-50%), translateY(-50%);\n}\n.select-icon, .input-box {\n  display: table-cell;\n}\n.input-box {\n  padding-left: 5px;\n  font-size: 20px;\n}\n.select-icon {\n  position: relative;\n  top: 7px;\n  left: 5px;\n  margin-right: 5px;\n  padding-right: 10px;\n  font-size: 25px;\n}\n.todo-div .todo-list {\n  top: 50px;\n  max-height: 400px;\n  background: #ccc;\n  overflow: auto;\n}\n.btn-list{\n  padding: 10px 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.btn-list button{\n  border: none;\n  font-size: inherit;\n}\n.btn-list button:hover{\n  border: 1px solid #A3A3A3;\n}\n.btn-list a:hover{\n  text-decoration: underline;\n  cursor: pointer;\n}\n</style>\n"]}]}